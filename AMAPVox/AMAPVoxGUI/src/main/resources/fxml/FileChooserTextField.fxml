<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.amap.lidar.amapvox.gui.FileChooserTextFieldController">
   <children>
      <Label fx:id="label" text="File">
         <VBox.margin>
            <Insets left="2.0" right="2.0" top="2.0" />
         </VBox.margin></Label>
      <HBox>
         <children>
            <TextField fx:id="textFieldFile" prefHeight="25.0" prefWidth="280.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="buttonOpenFile" mnemonicParsing="false" onAction="#onActionButtonOpenFile" text="..." />
         </children>
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
